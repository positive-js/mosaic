{
    "version": 2,
    "scope": "ptsecurity",
    "github": {
        "enabled": false
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        { "src": "/(.*).js", "dest": "/$1.js" },
        { "src": "/(.*).css", "dest": "/$1.css" },
        { "src": "/(.*).svg", "dest": "/$1.svg" },
        { "src": "/(.*).json", "dest": "/$1.json" },
        { "src": "/(.*).html", "dest": "/$1.html" }
    ]
}
