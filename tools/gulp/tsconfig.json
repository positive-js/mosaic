{
    "compilerOptions": {
        "experimentalDecorators": true,
        "noUnusedParameters": true,
        "lib": [
            "es2015",
            "dom",
            "es2016.array.include"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "../../dist/tools/gulp",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noEmitOnError": true,
        "noImplicitAny": true,
        "target": "es5",
        "types": [
            "node"
        ],
        "paths": {}
    },
    "files": [
        "gulpfile.ts"
    ]
}
