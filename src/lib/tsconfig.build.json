{
    "compilerOptions": {
        "declaration": true,
        "stripInternal": false,
        "experimentalDecorators": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "importHelpers": true,
        "newLine": "lf",
        "module": "es2015",
        "moduleResolution": "node",
        "outDir": "../../dist/packages/mosaic",
        "rootDir": ".",
        "rootDirs": [
            ".",
            "../../dist/packages/mosaic"
        ],
        "sourceMap": true,
        "inlineSources": true,
        "target": "es2015",
        "lib": ["es2015", "dom"],
        "skipLibCheck": true,
        "types": [],
        "baseUrl": ".",
        "paths": {
            "@ptsecurity/cdk/*": ["../../dist/packages/cdk/*"],
            "@ptsecurity/mosaic-moment-adapter/*": ["../../dist/packages/mosaic-moment-adapter/*"],
            "@ptsecurity/mosaic/*": ["../../dist/packages/mosaic/*"]
        }
    },
    "files": [
        "public-api.ts"
    ],
    "angularCompilerOptions": {
        "strictMetadataEmit": true,
        "flatModuleOutFile": "index.js",
        "flatModuleId": "@ptsecurity/mosaic",
        "skipTemplateCodegen": true,
        "fullTemplateTypeCheck": true
    }
}
